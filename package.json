{
  "name": "@storybook/addon-ie11",
  "version": "0.0.5",
  "description": "Storybook IE11 compatibility addon",
  "author": "tooppaaa",
  "keywords": [
    "storybook-addons",
    "ie11",
    "legacy",
    "compatibility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/addon-ie11"
  },
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"yarn storybook -- --no-manager-cache --quiet\" \"yarn build -- --watch\"",
    "build-storybook": "build-storybook",
    "release": "yarn build && auto shipit"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.11",
    "@storybook/node-logger": "*",
    "babel-loader": "^8.1.0",
    "event-source-polyfill": "^1.0.25"
  },
  "devDependencies": {
    "@storybook/react": "^6.4.0-alpha.30",
    "@types/babel__core": "^7.1.15",
    "@types/webpack": "^4.41.13",
    "auto": "^10.31.0",
    "chalk": "^4.1.2",
    "concurrently": "^6.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Addon IE 11",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "lit",
      "mithril",
      "marko",
      "svelte",
      "riot",
      "preact",
      "rax",
      "aurelia",
      "marionette"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
